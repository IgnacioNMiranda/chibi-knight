{
  "name": "chibi-knight",
  "version": "1.6.2",
  "description": "A multipurpose discord bot",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "build": "rm -rf ./dist && tsc",
    "builda": "rm -rf ./dist && tsc && tsc-alias",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"{src, test}/**/*.ts\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IgnacioNMiranda/Chibi-Knight"
  },
  "author": "Ignacio Miranda F. (N)",
  "bugs": {
    "url": "https://github.com/IgnacioNMiranda/Chibi-Knight/issues"
  },
  "homepage": "https://github.com/IgnacioNMiranda/Chibi-Knight#readme",
  "engines": {
    "node": "~16.13.1",
    "npm": "~8.1.2"
  },
  "types": "./dist/main.d.ts",
  "private": "true",
  "dependencies": {
    "discord.js": "^12.5.3",
    "discord.js-commando": "^0.12.3",
    "dotenv": "^10.0.0",
    "mongoose": "^6.1.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@typegoose/typegoose": "^9.4.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  }
}